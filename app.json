{
  "expo": {
    "name": "my-music-player",
    "slug": "my-music-player",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "WAKE_LOCK"
      ],
      "playback": {
        "android": {
          "stayAwake": true
        }
      },
      "package": "com.elsamplio2.mymusicplayer"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "69d2e99a-8506-4495-8370-2e4ebe4ba092"
      }
    }
  }
}
